[mariadb]
user                    = mysql

pid-file                = /run/mysqld/mysqld.pid

basedir                 = /usr
datadir                 = /var/lib/mysql
tmpdir                  = /tmp

lc-messages-dir         = /usr/share/mysql
lc-messages             = en_US

bind-address		    = *

[client-server]
port=3306

